DEFINE:		LCDCY		2500  # Length of CDC Yoke
DEFINE:		pZColl		-(-LColl/2+LCollS+LGapS+LCDCS/2+LTS3S) # Start from TS3 Section
DEFINE:		RHoleD		200  # Radius of the hole on the downstream end
#	CDC
